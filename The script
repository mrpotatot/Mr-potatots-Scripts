universalScripts:addButton("Headless", function()
scriptLibrary:Headless()
end)

local otherScripts= lastpage:addSection("Settings")

otherScripts:addButton("Destroy UI", function()
game.CoreGui["Mr potatot's scripts"]:Destroy()
end)
