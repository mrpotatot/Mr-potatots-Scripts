			local configFile = "";
			configFile = configFile .. "variables.espEnabled=" .. tostring(variables.espEnabled) .. "\n"
			configFile = configFile .. "variables.printerEspEnabled=" .. tostring(variables.printerEspEnabled) .. "\n"
