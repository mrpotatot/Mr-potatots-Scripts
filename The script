				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0);
				notice:Fire("Lunahax", "Teleported to baseplate");
			else
				notice:Fire("Lunahax", "Please enable death barrier bypass", 4);
			end
		end)
		
		
