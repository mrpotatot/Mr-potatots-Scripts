local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("Mr potatot's scripts", 5013109572)
local themes = {
Background = Color3.fromRGB(24, 24, 24),
Glow = Color3.fromRGB(0, 0, 0),
Accent = Color3.fromRGB(10, 10, 10),
LightContrast = Color3.fromRGB(20, 20, 20),
DarkContrast = Color3.fromRGB(14, 14, 14),
TextColor = Color3.fromRGB(255, 255, 255)
}
local page1 = venyx:addPage("Plane Crazy", 5012544693)
local page2 = venyx:addPage("Ragdoll Eng", 5012544693)
local page3 = venyx:addPage("Arsenal", 5012544693)
local page4 = venyx:addPage("Electric State", 5012544693)
local lastpage = venyx:addPage("Others", 5012544693)

local teleports = page1:addSection("Teleports")
teleports:addTextbox("TP to Base", "NameHere", function(value, focusLost)
for i,v in pairs(game.Players:GetChildren()) do
if (string.sub(string.lower(v.Name),1,string.len(value))) == string.lower(value) then
value = v.Name
end
end
if focusLost then
for i,v in pairs(game.Workspace.BuildingZones:GetChildren()) do
if tostring(v.Owner.Value) == value then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(v.Position.x, v.Position.y, v.Position.z - 75)
end
end
end
end)
local scripts1 = page1:addSection("Scripts")
scripts1:addTextbox("Kill Player(Needs Seat)", "NameHere", function(value, focusLost)
for i,v in pairs(game.Players:GetChildren()) do
if (string.sub(string.lower(v.Name),1,string.len(value))) == string.lower(value) then
value = v.Name
end
end
if focusLost then
for i,blocks in pairs(game.Workspace[tostring(game.Players.LocalPlayer) .. 'Aircraft']:GetChildren()) do
pcall(function()
if blocks.Name == "Seat" then
blocks.Seat.CFrame = game.Players[value].Character.HumanoidRootPart.CFrame
wait(0.8)
blocks.Seat.CFrame = CFrame.new(blocks.Seat.Position.x, (blocks.Seat.Position.y-5000), blocks.Seat.Position.z)
end
end)
end
end
end)
scripts1:addTextbox("Bring Player(Needs Seat)", "NameHere", function(value, focusLost)
for i,v in pairs(game.Players:GetChildren()) do
if (string.sub(string.lower(v.Name),1,string.len(value))) == string.lower(value) then
value = v.Name
end
end
if focusLost then
for i,blocks in pairs(game.Workspace[tostring(game.Players.LocalPlayer) .. 'Aircraft']:GetChildren()) do
if blocks.Name == "Seat" then
blocks.Seat.CFrame = game.Players[value].Character.HumanoidRootPart.CFrame
wait(0.8)
blocks.Seat.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
break
end
end
end
end)
scripts1:addTextbox("Trap Player(Needs Seat)", "NameHere", function(value, focusLost)
for i,v in pairs(game.Players:GetChildren()) do
if (string.sub(string.lower(v.Name),1,string.len(value))) == string.lower(value) then
value = v.Name
end
end
if focusLost then
for i,blocks in pairs(game.Workspace[tostring(game.Players.LocalPlayer) .. 'Aircraft']:GetChildren()) do
if blocks.Name == "Seat" then
blocks.Seat.CFrame = game.Players[value].Character.HumanoidRootPart.CFrame
break
end
end
end
end)
scripts1:addTextbox("Tnt Cluster To Player", "NameHere", function(value, focusLost)
for i,v in pairs(game.Players:GetChildren()) do
if (string.sub(string.lower(v.Name),1,string.len(value))) == string.lower(value) then
value = v.Name
end
end
if focusLost then
while true do
wait()
for i,blocks in pairs(game.Workspace[tostring(game.Players.LocalPlayer) .. 'Aircraft']:GetChildren()) do
pcall(function()
if blocks.Name == "ExplosiveBlock" then
task.defer(function()
while true do
blocks.Decorate.CFrame = game.Players[value].Character.HumanoidRootPart.CFrame
wait()
end
end)
end
if blocks.Name == "ExplosiveBall" then
task.defer(function()
while true do
blocks.Decorate.CFrame = game.Players[value].Character.HumanoidRootPart.CFrame
wait()
end
end)
end
end)
end
end
end
end)
scripts1:addButton('Bypass AntiCheat', function()
pcall(function()
game.ReplicatedStorage.Remotes.KickEvent:Destroy()
end)
end)
scripts1:addButton('Ocean Map', function()
local region = Region3.new(Vector3.new(-3750,-100,-2050), Vector3.new(2500,43,2700))
region = region:ExpandToGrid(4)
game.Workspace.Terrain:FillRegion(region, 4, Enum.Material.Water)
end)

scripts1:addButton('Physics Gun', function()
loadstring(game:HttpGet("https://pastebin.com/raw/6cfa5Wxa"))()
end)

local scripts2 = page2:addSection("Scripts")
scripts2:addButton("Cryptonic Hub", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/martinelcrac/cryptonichub/main/Ragdollengine.lua'))()
end)

local scripts3 = page3:addSection("Scripts")
scripts3:addButton("Arsenal Silent Aim", function()
loadstring(game:HttpGet('https://pastebin.com/raw/c6FDEdx8'))()
end)

local scripts4 = page4:addSection("Scripts")
scripts4:addButton("Luna Hax", function()
loadstring(game:HttpGet('https://pastebin.com/raw/ZP5x37sm'))()
end)

local scripts89908 = lastpage:addSection("Others")

scripts89908:addButton("Infinite Yield", function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
end)

scripts89908:addButton("Destroy UI", function()
game.CoreGui["Mr potatot's scripts"]:Destroy()
end)
